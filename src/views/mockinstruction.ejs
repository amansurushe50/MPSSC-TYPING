    <style>
        .table-container {
            padding: 30px; /* Adjust the padding value as needed */
        }
        .table {
            width: 100%;
            border-collapse: collapse;
        }
        .table th, .table td {
            padding: 10px;
            border: 1px solid #ddd;
        }
        .table-dark {
            background-color: #343a40;
            color: #fff;
        }
        .table-dark th, .table-dark td {
            border-color: #ffc107; /* Warning border color */
        }
         /* Additional styling for instructions */
         .instructions-container {
            background-color: #222;
            color: #fff;
            padding: 20px;
            margin-top: 20px;
            border-radius: 5px;
        }

        .instructions-title {
            font-weight: bold;
            margin-bottom: 10px;
        }

